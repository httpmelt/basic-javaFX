<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ku.cs.controllers.StudentListController">
   <children>
      <ListView fx:id="studentListView" layoutX="14.0" layoutY="60.0" prefHeight="298.0" prefWidth="315.0" />
      <Text layoutX="14.0" layoutY="45.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Student List">
         <font>
            <Font name="Arial" size="16.0" />
         </font>
      </Text>
      <Text layoutX="347.0" layoutY="73.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Selected Student" />
      <Text layoutX="347.0" layoutY="109.0" strokeType="OUTSIDE" strokeWidth="0.0" text="ID: " />
      <Text layoutX="345.0" layoutY="142.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Name:" />
      <Text layoutX="345.0" layoutY="178.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Score:" />
      <Label fx:id="idLabel" layoutX="423.0" layoutY="96.0" text="idLabel" />
      <Label fx:id="nameLabel" layoutX="423.0" layoutY="129.0" text="nameLabel" />
      <Label fx:id="scoreLabel" layoutX="423.0" layoutY="165.0" text="scoreLabel" />
      <Button layoutY="2.0" mnemonicParsing="false" onAction="#onBackButtonClick" text="Back" />
      <Text layoutX="345.0" layoutY="214.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Give score:" />
      <TextField fx:id="giveScoreTextField" layoutX="423.0" layoutY="197.0" prefHeight="26.0" prefWidth="41.0" />
      <Button layoutX="480.0" layoutY="197.0" mnemonicParsing="false" onAction="#onGiveScoreButtonClick" prefHeight="26.0" prefWidth="41.0" text="OK" />
      <Label fx:id="errorLabel" layoutX="423.0" layoutY="223.0" text="errorLabel" textFill="RED">
         <font>
            <Font size="10.0" />
         </font>
      </Label>
      <Button layoutX="454.0" layoutY="306.0" mnemonicParsing="false" onAction="#onNextTableViewButtonClick" text="Table View" />
   </children>
</AnchorPane>
